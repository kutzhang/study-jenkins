@Library("jsl") _

pipeline {
    agent any

    options {
        timestamps()
    }
    
    stages {
        stage('Send message') {
            steps {
                echo 'hahahahahah'
                sendMessage(this) {
                    title 'Demo Title'
                    receiver 'Lana Chang'
                    sender 'Kut Zhang'
                }
            }
        }
    }
}