@Library("jsl") _

pipeline {
    agent any
    
    stages {
        stage('Send message') {
            steps {
                echo 'hahahahahah'
                sendMessage(this) {
                    title 'Demo Title'
                    content 'Demo Content'
                    receiver 'Lana Chang'
                    sender 'Kut Zhang'
                }
            }
        }
    }
}