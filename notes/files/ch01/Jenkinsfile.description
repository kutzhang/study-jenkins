pipeline {
    agent any

    options {
        timestamps()
        disableConcurrentBuilds()
        timeout(time: 30, unit: 'MINUTES')
    }

    stages {
        stage('Complete') {
            steps {
                echo 'Processing.....'
                echo 'Complete'
            }
        }
    }

    post {
        success {
            script {
                currentBuild.description = 'Complete'
            }
        }
    }
}
