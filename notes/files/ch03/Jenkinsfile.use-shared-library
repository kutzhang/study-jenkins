@Library("study")
import com.sanlea.study.jenkins.MessageManager

pipeline {
    agent {
        label "maven"
    }

    stages {
        stage('standard class') {
            steps {
                script {
                    MessageManager.say this, "how do you turn this on?"
                }
            }
        }

        stage('vars') {
            steps {
                sayHello this, 'show me the money!'
            }
        }
    }
}
